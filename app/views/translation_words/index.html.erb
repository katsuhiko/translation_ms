<div class="page-header">
  <%= link_to new_translation_word_path, :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-plus"></span>
    New Translation word
  <% end %>
  <h1>Listing translation_words</h1>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
            <th>Prev</th>
            <th>Cud type</th>
            <th>Work</th>
            <th>Locale</th>
            <th>Content</th>
            <th>Progress status</th>
            <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>

    <tbody>
      <%= content_tag_for(:tr, @translation_words) do |translation_word| %>
            <td><%= translation_word.prev_id %></td>
            <td><%= translation_word.cud_type %></td>
            <td><%= translation_word.work %></td>
            <td><%= translation_word.locale %></td>
            <td><%= translation_word.content %></td>
            <td><%= translation_word.progress_status %></td>
            <td><%= link_to 'Show', translation_word %></td>
        <td><%= link_to 'Edit', edit_translation_word_path(translation_word) %></td>
        <td><%= link_to 'Destroy', translation_word, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      <% end %>
    </tbody>
  </table>
</div>
