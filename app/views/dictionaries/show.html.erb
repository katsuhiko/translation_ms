<div class="page-header">
  <%= link_to dictionaries_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_dictionary_path(@dictionary), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show dictionary</h1>
</div>

<dl class="dl-horizontal">
  <dt>Name:</dt>
  <dd><%= @dictionary.name %></dd>

  <dt>Description:</dt>
  <dd><%= @dictionary.description %></dd>

</dl>
